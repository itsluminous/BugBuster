{
   "background": {
      "page": "html/background-page.html"
   },
   "browser_action": {
      "default_icon": {
         "19": "img/rec-start.png",
         "38": "img/rec-start.png"
      },
      "default_popup": "html/popup.html"
   },
   "description": "Screencast and Network Debugging Tool",
   "icons": {
      "128": "img/bugbuster_128.transparent.png"
   },
   "manifest_version": 2,
   "name": "BugBuster",
   "permissions": [ "\u003Call_urls>", "activeTab", "alarms", "storage", "notifications", "tabs", "webNavigation", "webRequest", "debugger", "tabCapture", "desktopCapture" ],
   "short_name": "BugBuster",
   "version": "3.0.17",
  "web_accessible_resources": [
    "resources/*"
  ]
}
