{
   "background": {
      "page": "html/background-page.html"
   },
   "browser_action": {
      "default_icon": {
         "19": "img/bugreplay_19.png",
         "38": "img/bugreplay_38.png"
      },
      "default_popup": "html/popup.html"
   },
   "description": "Screencast and Network Debugging Tool",
   "externally_connectable": {
      "matches": [ "https://app.bugreplay.com/*", "https://rc-dot-bugreplay-client.appspot.com/*", "http://local.bugreplay/*", "http://dev.bugreplay.com/*", "https://bugreplay-site-dev.appspot.com/*" ]
   },
   "icons": {
      "128": "img/bugreplay_128.transparent.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "BugReplay",
   "permissions": [ "\u003Call_urls>", "activeTab", "alarms", "storage", "notifications", "tabs", "webNavigation", "webRequest", "debugger", "tabCapture", "desktopCapture" ],
   "short_name": "BugReplay",
